{
  "name": "onirux",
  "private": true,
  "version": "0.0.2",
  "type": "module",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "concurrently \"vite\" \"tsc -w --project electron/tsconfig.json\" \"tsc -w --project electron/tsconfig.preload.json\" \"wait-on http://localhost:5173 && electron .\"",
